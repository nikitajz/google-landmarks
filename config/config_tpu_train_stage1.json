{
  "model_name": "resnet101",
  "loss_module": "arcface",
  "margin": 0.15,
  "use_fc": true,
  "freeze_backbone": false,
  "log_dir": "logs/",
  "resume_checkpoint": null,
  "checkpoints_dir": "logs/checkpoints/classification/",
  "data_path": "data/x256",
  "data_train": "train.csv",
  "test_size": 96000,
  "min_class_samples": 20,
  "image_size": 256,
  "crop_size": 224,
  "use_weighted_sampler": true,
  "limit_samples_to_draw": 512000,
  "num_workers": 6,
  "tpu_cores": 1,
  "n_epochs": 20,
  "batch_size": 64,
  "accumulate_grad_batches": 1,
  "optimizer": "SGD",
  "lr": 0.1,
  "momentum": 0.9,
  "weight_decay": 0.0001,
  "scheduler": "reduce_lr_on_plateau",
  "factor": 0.1,
  "patience": 2,
  "val_check_interval": 0.1
}
